{
  "manifest_version": 2,
  "name": "Send to ZeroNote",
  "version": "1.0",

  "description": "Send web pages to ZeroNote from Chrome directly",
  "icons": {
    "512": "notebook.png"
  },

  "permissions": ["contextMenus", "storage"],
  "background": {
    "scripts": ["zepto.min.js", "background.js"]
  },
  "browser_action": {
    "default_popup": "options.html"
  }
}
